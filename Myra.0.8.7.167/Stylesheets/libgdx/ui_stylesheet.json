{
    "colors": {
        "white": "#FFFFFFFF",
        "grey": "#AAAAAAFF"
    },
	"textureAtlases": {
		"ui_stylesheet_atlas.json": "ui_stylesheet_atlas.png"
	},
	"fonts": {
		"ui_font": "ui_font.fnt"
	},
	
    "textBlock": {
        "font": "ui_font",
        "disabledTextColor": "grey",
        "textColor": "white"
    },
    "textField": {
        "font": "ui_font",
        "textColor": "white",
        "disabledTextColor": "grey",
        "selection": "selection",
        "background": "textfield",
        "cursor": "cursor",
        "messageFont": "ui_font",
        "messageTextColor": "grey"
    },
    "scrollArea": {
        "background": "default-rect",
        "horizontalScroll": "default-scroll",
        "horizontalScrollKnob": "default-round-large",
        "verticalScroll": "default-scroll",
        "verticalScrollKnob": "default-round-large"
    },
    "button": {
        "padding": {
            "left": 5.0,
            "right": 5.0
        },
        "background": "default-round",
        "pressedBackground": "default-round-down",
        "label": {
            "font": "ui_font",
            "textColor": "white",
            "disabledTextColor": "grey"
        }
    },
    "imageButton": {
        "padding": {
            "left": 5.0,
            "right": 5.0
        },
        "background": "default-round",
        "pressedBackground": "default-round-down"
    },
    "textButton": {
        "padding": {
            "left": 5.0,
            "right": 5.0
        },
        "background": "default-round",
        "pressedBackground": "default-round-down",
        "label": {
            "font": "ui_font",
            "textColor": "white",
            "disabledTextColor": "grey"
        }
    },
    "checkBox": {
        "label": {
            "font": "ui_font",
            "textColor": "white"
        },
        "image": {
            "image": "check-off",
            "pressedImage": "check-on"
        }
    },
    "spinButton": {
        "background": "textfield",
        "upButton": {
            "background": "default-round",
            "pressedBackground": "default-round-down",
            "image": {
                "image": "tree-plus"
            }
        },
        "downButton": {
            "background": "default-round",
            "pressedBackground": "default-round-down",
            "image": {
                "image": "tree-minus"
            }
        },
        "textField": {
            "font": "ui_font",
            "textColor": "white",
            "disabledTextColor": "grey",
            "selection": "selection",
            "cursor": "cursor",
            "messageFont": "ui_font",
            "messageTextColor": "grey"
        }
    },
    "horizontalSlider": {
        "background": "default-slider",
        "knob": {
            "image": {
                "image": "default-slider-knob"
            }
        }
    },
    "verticalSlider": {
        "background": "default-slider",
        "knob": {
            "image": {
                "image": "default-slider-knob"
            }
        }
    },
    "horizontalProgressBar": {
        "background": "default-round",
        "filled": "default-round-down"
    },
    "verticalProgressBar": {
        "background": "default-round",
        "filled": "default-round-down"
    },
    "comboBox": {
        "background": "default-select",
        "label": {
            "font": "ui_font",
            "textColor": "white"
        },
        "itemsContainer": {
            "background": "default-rect"
        },
        "comboBoxItem": {
            "pressedBackground": "default-select-selection",
            "label": {
                "font": "ui_font",
                "textColor": "white"
            }
        }
    },
    "listBox": {
        "background": "default-rect",
        "listBoxItem": {
            "pressedBackground": "default-select-selection",
            "padding": {
                "left": 5.0,
                "right": 5.0
            },
            "label": {
                "font": "ui_font",
                "textColor": "white",
                "disabledTextColor": "grey"
            }
        }
    },
    "tree": {
        "rowSelectionBackground": "default-select-selection",
        "mark": {
            "image": {
                "image": "tree-plus",
                "pressedImage": "tree-minus"
            },
            "label": {
                "font": "ui_font",
                "textColor": "white",
                "disabledTextColor": "grey"
            }
        }
    },
    "horizontalSplitPane": {
        "handle": {
            "background": "default-splitpane"
        }
    },
    "verticalSplitPane": {
        "handle": {
            "background": "default-splitpane-vertical"
        }
    },
    "horizontalMenu": {
        "background": "default-round",
        "separator": {
            "image": "default-round",
            "thickness": 3
        },
        "menuItem": {
            "pressedBackground": "default-round-down",
            "openBackground": "default-round-down",
            "padding": {
                "left": 5.0,
                "right": 5.0,
                "bottom": 2.0
            },
            "label": {
                "font": "ui_font",
                "textColor": "white",
                "disabledTextColor": "grey"
            }
        }
    },
    "verticalMenu": {
        "background": "default-round",
        "separator": {
            "image": "default-round",
            "thickness": 4,
            "padding": {
                "top": 2.0,
                "bottom": 2.0
            }
        },
        "menuItem": {
            "pressedBackground": "default-round-down",
            "iconWidth": 22,
            "shortcutWidth": 22,
            "padding": {
                "left": 5.0,
                "right": 5.0,
                "top": 2.0,
                "bottom": 2.0
            },
            "label": {
                "font": "ui_font",
                "textColor": "white",
                "disabledTextColor": "grey"
            }
        }
    },
    "window": {
        "background": "default-window",
        "padding": {
            "left": 2.0,
            "right": 2.0
        },
        "title": {
            "padding": {
                "left": 5.0,
                "right": 5.0
            },
            "font": "ui_font",
            "textColor": "white"
        },
        "closeButton": {
            "image": {
                "image": "default-round",
                "pressedImage": "default-round-down"
            }
        }
    }
}